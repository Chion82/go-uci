{
  "go.useLanguageServer": true,
  "go.formatTool": "goimports",
  "go.lintTool": "golangci-lint",
  "go.lintFlags": [
    "--fast",
    "--enable-all",
    "--disable=errcheck",
    "--disable=gas",
    "--disable=gochecknoglobals",
    "--disable=gochecknoinits",
    "--disable=gocyclo",
    "--disable=lll",
    "--disable=nakedret",
  ],
  "go.testFlags": ["-v"]
}
